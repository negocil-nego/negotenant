type Tenant {
    uuid: ID!
    name: String!
}

type TenantPage {
    content: [Tenant]!
    empty: Boolean!
    first: Boolean!
    last: Boolean!
    number: Int!
    numberOfElements: Int!
    size: Int!
    totalElements: Int!
    totalPages: Int!
}

input TenantDto {
    name: String!
    code: String!
}

extend type Query {
    paginateTenant(paginateDto: PaginateDto): TenantPage!
}

extend type Mutation {
    saveTenant(tenantDto: TenantDto!): Tenant
}